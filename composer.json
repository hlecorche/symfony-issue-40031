{
    "name": "issue/mybundle",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "doctrine/doctrine-bundle": "^2.0.3",
        "doctrine/orm": "^2.6.3",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/doctrine-bridge": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": { "Issue\\MyBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Issue\\MyBundle\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
